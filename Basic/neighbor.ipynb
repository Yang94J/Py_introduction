   "source": [
    "import copy\n",
    "def inrange(a,b,li):\n",
    "    count=0\n",
    "    for i in li:\n",
    "        if i>=a-b and i<=a+b:\n",
    "            count+=1\n",
    "    print(count)\n",
    "b=float(input(\"please enter the radius:\"))\n",
    "li=[-3,2,9,90]\n",
    "for i in li:\n",
    "    a=copy.copy(i)\n",
    "    inrange(a,b,li)"
   ]
