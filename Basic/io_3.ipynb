   ],
   "source": [
    "n,m=input(\"please enter row and column seperately:\").split()\n",
    "n,m=int(n),int(m)\n",
    "Li2=[]\n",
    "for i in range(n):\n",
    "    print(\"please enter \",m,\" numbers for\",i+1,\"row\")\n",
    "    Li=input().split()\n",
    "    Li2.append(Li)\n",
    "print(Li2)"
   ]
