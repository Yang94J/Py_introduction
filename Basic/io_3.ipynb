   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "please enter row and column seperately:3 4\n",
      "please enter  4  numbers for 1 row\n",
      "1 2 3 4\n",
      "please enter  4  numbers for 2 row\n",
      "2 3 4 5\n",
      "please enter  4  numbers for 3 row\n",
      "3 4 5 6\n",
      "[['1', '2', '3', '4'], ['2', '3', '4', '5'], ['3', '4', '5', '6']]\n"
     ]
